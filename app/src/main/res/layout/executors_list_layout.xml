<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    style="@style/BaseLL"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout style="@style/HeaderLL">

        <LinearLayout
            style="@style/Image_Wrap"
            android:layout_width="60dp">

            <ImageView
                android:id="@+id/ExecutorsList_BackIV"
                style="@style/Back_Arrow_IV"/>

        </LinearLayout>
        <View style="@style/One_Row_Strut"/>
        <TextView
            android:id="@+id/ExecutorsList_HeaderTitleTV"
            style="@style/HeaderTitle_TV"/>
        <View style="@style/One_Row_Strut"/>
        <LinearLayout
            style="@style/Image_Wrap"
            android:layout_width="50dp">

            <View style="@style/One_Row_Strut" />
            <ImageView
                android:id="@+id/ExecutorsList_FindIV"
                style="@style/Find_IV"/>

        </LinearLayout>
        <LinearLayout
            style="@style/Image_Wrap">

            <ImageView
                android:id="@+id/ExecutorsList_SetUserDataIV"
                style="@style/RootHashTag_SetUserData_IV"/>

        </LinearLayout>

    </LinearLayout>
    <ScrollView style="@style/MW">

        <LinearLayout
            android:id="@+id/ExecutorsList_ExecutorsLL"
            style="@style/ExecutorsList_Executors_LL">

        </LinearLayout>

    </ScrollView>

</LinearLayout>